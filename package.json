{
	"name": "@jee-r/directus-extension-audio-metadata",
	"description": "Get remote audio metadata from directus flow",
	"icon": "extension",
	"author": "Jee R",
	"repository": "github.com/jee-r/directus-extension-audio-metadata",
	"homepage": "https://github.com/jee-r/directus-extension-audio-metadata",
	"license": "MIT",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-operation"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"changelog": "conventional-changelog --config ./changelog-config.mjs -i CHANGELOG.md -s",
		"version": "conventional-changelog --config ./changelog-config.mjs -i CHANGELOG.md -s && git add CHANGELOG.md"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.0.4",
		"@types/node": "^22.13.17",
		"conventional-changelog-cli": "^5.0.0",
		"conventional-changelog-conventionalcommits": "^8.0.0",
		"typescript": "^5.8.2",
		"vue": "^3.5.13"
	},
	"dependencies": {
		"music-metadata": "^11.0.3"
	}
}
